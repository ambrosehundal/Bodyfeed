p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Star rating</th>
      <th>Accomodation type</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event["name"] %></td>
        <td><%= event["status"]%></td>
        <td><%= event["city"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

