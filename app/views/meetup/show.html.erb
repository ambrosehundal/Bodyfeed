<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table class="table-bordered">
  <thead>
    <tr>
      <th>Name</th>
      <th>Status</th>
      <th>Date</th>
      <th>Local Time</th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event["name"] %></td>
        <td><%= event["status"]%></td>
        <td><%= event["local_date"] %></td>
        <td><%= event["local_time"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

